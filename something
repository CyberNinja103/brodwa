if not getgenv()["Animator"] then
	loadstring(game:HttpGet("https://raw.githubusercontent.com/xhayper/Animator/main/Source/Main.lua"))()
	hookAnimatorFunction()
end
local Anim
game.Players.LocalPlayer.PlayerScripts:WaitForChild("PlayerModule"):WaitForChild("CameraModule"):WaitForChild("MouseLockController"):WaitForChild("BoundKeys").Value = "LeftControl"
local id
local walk = 109130982296927
local mouse1 = false
local finish = false
local i = 138754221537146
local m1 = 83829782357897
local fid = 89669118763885
local sprinting1 = false
local pizza = false
local pizzaid = 116814116277716
local ability = false
local rtrid = 116996902830888
local rtr = false
local eye = false
local eyeid = 119429069577280
local entanglement = false
local CNid = 86799093901669
local run = 106485518413331
local uis = game:GetService("UserInputService")
local walking = false
local hwalking = 132377038617766
local hurt = false
local hidle = 134624270247120
local hrun = 115946474977409
local sprinting = false
local taunt = false
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local mouse = player:GetMouse()
local idle = true
local char = game.Players.LocalPlayer.Character
local hum = char:FindFirstChild("Humanoid")
hum.Animator:Destroy()
char.Animate:Destroy()
Anim = hum:LoadAnimation(i)
Anim:Play()
id = i
hum.WalkSpeed = 6
uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.LeftShift and entanglement == false and pizza == false then
	sprinting1 = true
		hum.WalkSpeed = 30
	end
	end)
uis.InputEnded:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.LeftShift and entanglement == false and pizza == false then
	    sprinting1 = false
	    hum.WalkSpeed = 6
	end
	end)
uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.One then
        Anim:Stop()
        id = "taunt"
        Anim = hum:LoadAnimation(111309618966926)
        taunt = true
        Anim:Play()
	end
end)
uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.T and taunt == false and pizza == false then
        if id ~= rtrid then 
        hum.WalkSpeed = 0
        sprinting1 = false
        ability = true
        Anim:Stop(2)
		Anim = hum:LoadAnimation(rtrid)
		Anim.Looped = false
		Anim:Play()
        id = rtrid
        wait(4)
        ability = false
        hum.WalkSpeed = 6
        
        
        end
	end
end)
uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.Q and pizza == false and finish == false then
        sprinting1 = false
        wait(.1)    
        entanglement = true
        hum.WalkSpeed = 0
        wait(.5)
        entanglement = false
        hum.WalkSpeed = 9
	end
end)
uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.R and entanglement == false and finish == false  then
        sprinting1 = false
        wait(.1)    
        pizza = true
        hum.WalkSpeed = 0
        wait(2.6)
        pizza = false
        hum.WalkSpeed = 9
	end
end)
uis.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.E and entanglement == false  then
        sprinting1 = false
        ability = true
        eye = true
        hum.WalkSpeed = 0
        wait(1)
        ability = false
        eye = false
        hum.WalkSpeed = 9
	end
end)
local function onButton1Down()
    if id ~= m1 then 
        mouse1 = true
        Anim:Stop()
		Anim = hum:LoadAnimation(m1)
		Anim.Looped = false
		Anim:Play()
		id = m1
		wait(.5)
		mouse1 = false
		end
end

mouse.Button1Down:Connect(onButton1Down)
while wait() do
	if idle == true and taunt == false and hurt == false and ability == false and entanglement == false and pizza == false and mouse1 == false and finish == false then
		if id ~= i then 
	    Anim:Stop(2)
		Anim = hum:LoadAnimation(i)
		Anim.Looped = true
		Anim:Play()
		id = i
		end
	end
	if walking == true and taunt == false and ability == false and hurt == false and sprinting1 == false and entanglement == false and pizza == false and finish == false and mouse1 == false then
		if id ~= walk then
		Anim:Stop(2)
		Anim = hum:LoadAnimation(walk)
		Anim.Looped = true
		Anim:Play() 
		id = walk
		end
	end
	if sprinting == true and taunt == false and ability == false and hurt == false and mouse1 == false then
		if id ~= run then
        Anim:Stop(2)
		Anim = hum:LoadAnimation(run)
		Anim.Looped = true
		Anim:Play() 
		id = run
		end
	end
	if hum.MoveDirection ~= Vector3.new(0, 0, 0) and sprinting1 == false then
		walking = true
		sprinting = false
		idle = false
		taunt = false
	end
	if hum.MoveDirection ~= Vector3.new(0, 0, 0) and sprinting1 == true and mouse1 == false and finish == false  then
		walking = false
		sprinting = true
		idle = false
		taunt = false
	end
	if hum.MoveDirection == Vector3.new(0,0,0) then
		idle = true
		walking = false
		sprinting = false
	end
	if idle == true and taunt == false and mouse1 == false and hurt == true and entanglement == false and pizza == false and finish == false  then
		if id ~= hidle then 
	    Anim:Stop(2)
		Anim = hum:LoadAnimation(hidle)
		Anim.Looped = true
		Anim:Play()
		id = hidle
		end
	end
	if walking == true and taunt == false and mouse1 == false and hurt == true and sprinting1 == false and attack == false and pizza == false and finish == false  then
		if id ~= hwalking then
		Anim:Stop(2)
		Anim = hum:LoadAnimation(hwalking)
		Anim.Looped = true
		Anim:Play() 
		id = hwalking
		end
	end
	if sprinting == true and taunt == false and hurt == true and mouse1 == false then
		if id ~= hrun then
        Anim:Stop(2)
		Anim = hum:LoadAnimation(hrun)
		Anim.Looped = true
		Anim:Play() 
		id = hrun
		end
	end
	if pizza == true and taunt == false then
		if id ~= pizzaid then 
	    Anim:Stop()
		Anim = hum:LoadAnimation(pizzaid)
		Anim.Looped = false
		Anim:Play()
		id = pizzaid
		end
	end
	if entanglement == true and taunt == false then
		if id ~= CNid then 
	    Anim:Stop()
		Anim = hum:LoadAnimation(CNid)
		Anim.Looped = false
		Anim:Play()
		id = CNid
		end
	end
		if eye == true and taunt == false then
		if id ~= eyeid then 
	    Anim:Stop()
		Anim = hum:LoadAnimation(eyeid)
		Anim.Looped = false
		Anim:Play()
		id = eyeid
		end
		end
	if rtr == true and taunt == false then
		if id ~= rtrid then 
	    Anim:Stop()
		Anim = hum:LoadAnimation(rtrid)
		Anim.Looped = false
		Anim:Play()
		id = rtrid
		end
	end

 end

